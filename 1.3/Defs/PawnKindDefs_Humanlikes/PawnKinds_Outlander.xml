    <PawnKindDef ParentName="OutlanderBase">
    <defName>Doctor</defName>
    <label>doctor</label>
    <combatPower>100</combatPower>
	<backstoryFiltersOverride>
      <li>
        <categories>
          <li>Outlander</li>
		            <li>Nurse</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
		    <defaultFactionType>OutlanderCivil</defaultFactionType>
	    <requiredWorkTags>
